import java.util.*;
class Main
{
    public static void main(String []args)
    {
        Scanner sc=new Scanner(System.in);
        int m=sc.nextInt();
        int n=sc.nextInt();
        int a=0;
        int b=0;
        if(m>n){
           b=m;
           a=n;
        }
        else{
          a=m;
          b=n;
        }
        for(int i=a;i<b;i++){
          System.out.print(i+"*"+(i+1)+", ");
          
        }
        System.out.print(b+"*"+(b+1));
        System.out.println();
        for(int i=a;i<b;i++){
          System.out.print(i*(i+1)+", ");
        }
        System.out.print(b*(b+1));
    }
}
