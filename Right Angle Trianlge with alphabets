import java.util.*;
class Main{
  public static void main(String[] args){
    Scanner sc=new Scanner(System.in);
    int n=Math.abs(sc.nextInt());
    if(n==0){
      System.out.print("Invalid Input");
    }
    else if(n>=8){
      System.out.print("Range Exceeded");
    }
    else{
    char a=65;
    for(int i=1;i<=n;i++){
      for(int j=1;j<=i;j++){
        System.out.print(a+" ");
        a++;
      }
      System.out.println();
    }
    }
  }
}
